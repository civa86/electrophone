    "redux": "3.1.7",
    "react-redux": "4.1.2",
    "redux-thunk": "1.0.3",
    "lodash": "4.0.1",
    "jquery": "1.12.1",
    "jquery-knob": "1.2.11",
    "ionicons": "2.0.1",
    "cytoscape": "2.5.5",
    "bootstrap": "3.3.6"
  },
  "devDependencies": {
    "babel-cli": "6.3.17",
    "babel-core": "6.3.26",
    "babel-loader": "6.2.2",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015-loose": "6.1.4",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "chai": "3.5.0",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",

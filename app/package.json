    "webpack": "1.9.13",
    "webpack-dev-server": "1.9.0"
  },
  "scripts": {
    "test": "mocha ./src/**/*.spec.js ./src/components/**/*.spec.js --reporter spec --compilers js:babel-core/register --recursive --colors",
    "test::watch": "npm test -- --watch",
    "app::dev": "node server.js",
    "app::build": "rm -rf ./dist; NODE_ENV=production webpack --config webpack.config.js --progress --colors --profile --bail",
    "app::dist": "npm test -- --bail && npm run app::build && cd .. && npm run lib::docs::public"
  }
